include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main(){
	int arr[4] = {9,8,7,6};
	int size = 4;
	int temp = arr[0];

	for(int i=0;i<4;i++){
		if(i < size-1){
	
		arr[i]=arr[i+1];
	
	}
		else{
			arr[size-1] = temp;
		
		}
	
	}

	for(int i=0;i<size;i++){
	printf("%d  ",arr[i]);
	}



	return 0;
    }   
 
